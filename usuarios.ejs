<!DOCTYPE html>
<html>
    <head>
        <% include components/projectDefaultHead/projectDefaultHead %>
    </head>
    <body>
        <% include components/header/projectHeaderLogado %>
        <main>
         <div>
        <table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nome</th>
      <th scope="col">Email</th>
      <th scope="col">Status</th>
      <th scope="col">Opção</th>

    </tr>
  </thead>
  <% membros.forEach( membro => { %>
  <tbody>
    <tr>
      <th scope="row"><%= membro.id%></th>
      <td><%= membro.nome %></td>
      <td><%= membro.email %></td>
      <td><%= membro.status %></td>
      <td><button type="button" class="btn btn-danger">Deletar</button></td>
    </tr>
  </tbody>
  <% }); %>
</table>
      </div>
        </main>
        <% include components/footer/projectDefaultFooter %>

        <% include components/projectDefaultScripts/projectDefaultScripts %>
    </body>
</html>